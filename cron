#Every Min
#* * * * * php /data/project/addbot/enwiki/run.php

*/3 * * * * php /data/project/addbot/enwiki/run.wd.php

#Every 5 Mins
#The below kills any run.php that has been running for over an hour wusing over 100,000kb of ram
*/5 * * * * ps -o sz,etime,pid,comm,args -U addshore | grep run.php | grep -v grep | perl -ane '($m,$s) = split /:/,$F[1]; $kb=$F[0]; $pid = $F[2]; if(($m>60)||($kb>100000)) {print "$m :: $s $kb $pid\n"; kill 9, $pid;}'
*/5 * * * * ps -o sz,etime,pid,comm,args -U addshore | grep list.php | grep -v grep | perl -ane '($m,$s) = split /:/,$F[1]; $kb=$F[0]; $pid = $F[2]; if(($m>240)||($kb>100000)) {print "$m :: $s $kb $pid\n"; kill 9, $pid;}'
*/5 * * * * ps -o sz,etime,pid,comm,args -U addshore | grep run.wd.php | grep -v grep | perl -ane '($m,$s) = split /:/,$F[1]; $kb=$F[0]; $pid = $F[2]; if(($m>60)||($kb>100000)) {print "$m :: $s $kb $pid\n"; kill 9, $pid;}'

